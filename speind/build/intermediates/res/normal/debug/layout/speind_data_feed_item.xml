<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_wrap"
    android:layout_width="match_parent"
    android:layout_height="56dp"
    android:background="@drawable/clickable"
    android:clickable="true"
    android:minHeight="48dp" >
	
   <RelativeLayout
       android:id="@+id/item_delimeter_wrap"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:layout_marginLeft="16dp"
       android:layout_marginRight="16dp"
       android:background="@drawable/delimeter_h" >

	   <ToggleButton
		   android:id="@+id/enable_plugin"
		   android:layout_width="56dp"
		   android:layout_height="56dp"
		   android:layout_alignParentRight="true"
           android:checked="false"
		   android:textOff=""
		   android:textOn=""
           android:layout_centerVertical="true"
           android:clickable="true" />

	    <Button
	        android:id="@+id/settings_button"
	        android:layout_width="56dp"
	        android:layout_height="56dp"
	        android:layout_alignParentRight="true"
	        android:layout_centerVertical="true"
	        android:background="@drawable/btn_arrow"
	        android:visibility="invisible"
			style="?android:attr/buttonBarButtonStyle"/>
	
	    <ImageView
	        android:id="@+id/logo"
	        android:layout_width="48dp"
	        android:layout_height="48dp"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:src="@drawable/ic_launcher" />
	
	    <TextView
	        android:id="@+id/title"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_centerVertical="true"
	        android:layout_marginLeft="14dp"
	        android:layout_toLeftOf="@+id/settings_button"
	        android:layout_toRightOf="@+id/logo"
	        android:textColor="#383939"
	        android:textSize="@dimen/text14sp" />
   </RelativeLayout>
</RelativeLayout><!-- From: file:/C:/Programmer/Projects/AndroidstudioProjects/Speind/speind/src/main/res/layout/speind_data_feed_item.xml -->